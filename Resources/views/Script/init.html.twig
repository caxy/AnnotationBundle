<link href="{{ asset([base_url, 'bundles/caxyannotation/annotator/annotator.min.css']|join) }}" type="text/css" rel="stylesheet" />
<script type="text/javascript" src="{{ asset([base_url, 'bundles/caxyannotation/annotator/annotator.min.js']|join) }}"></script>

{# Plugins #}
{% if 'store' in plugin_array %}
	<script type="text/javascript" src="{{ asset([base_url, 'bundles/caxyannotation/annotator/annotator.store.min.js']|join) }}"></script>
{% endif %}

{# Init #}
<script type="text/javascript" src="{{ asset([base_url, 'bundles/caxyannotation/js/init.standard.js']|join) }}"></script>

<script type="text/javascript">
	initAnnotator({{ annotation_config | raw}},  '{{- app.request.getBaseUrl() -}}');
</script>